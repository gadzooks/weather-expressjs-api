{
  "name": "weather-expressjs",
  "version": "0.0.1",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build:clean && npm run build:typescript && npm run build:static",
    "build:clean": "rimraf dist",
    "build:static": "cpx \"src/**/*.{yml,json}\" dist && cpx \"tests/**/*.{yml,json}\" dist",
    "build:typescript": "npm run tsc -- --project tsconfig.build.json",
    "cpx": "cpx \"src/**/*.{html,png,jpg}\" dist && cpx \"tests/**/*.{html,png,jpg}\" dist",
    "heroku-prebuild": "npm config set ignore-scripts true",
    "lint": "eslint ./src --ext .ts --fix",
    "prestart": "npm run build",
    "pretest": "npm run build:static",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "production": "node -r dotenv/config dist/index.js",
    "start": "cross-env NODE_ENV=development concurrently \"npm run tsc -- --project tsconfig.build.json --watch\" \"nodemon -r dotenv/config -q --inspect dist/index.js\"",
    "test": "cross-env NODE_ENV=test jest --testTimeout=5000",
    "test:coverage": "jest --coverage",
    "watch": "jest --watch",
    "tsc": "tsc"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.5",
    "aws-serverless-express": "^3.4.0",
    "axios": "^1.6.8",
    "body-parser": "^1.20.2",
    "cache-manager": "^5.4.0",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "debug": "~4.3.4",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-cache-middleware": "^1.0.1",
    "helmet": "^7.1.0",
    "http-errors": "~2.0.0",
    "js-yaml": "^4.1.0",
    "morgan": "~1.10.0",
    "rimraf": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.23.3",
    "@types/cache-manager": "^4.0.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/http-errors": "^2.0.4",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.28",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.2"
  }
}
