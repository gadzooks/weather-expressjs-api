{
  "name": "weather-expressjs",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "preserve": "npm run build && cpx \"src/**/*.yml\" dist",
    "cpx": "cpx \"src/**/*.{html,png,jpg}\"",
    "serve": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon -q dist/index.js\"",
    "prestart": "npm run build && cpx \"src/**/*.yml\" dist",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "scripts-old": {
    "start": "tsc && node ./src/app.js",
    "build": "npx tsc",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon ./bin/www\""
  },
  "dependencies": {
    "@types/body-parser": "^1.19.2",
    "body-parser": "^1.20.0",
    "cookie-parser": "~1.4.4",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "express": "~4.16.1",
    "helmet": "^5.1.0",
    "http-errors": "~1.6.3",
    "js-yaml": "^4.1.0",
    "morgan": "~1.9.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/js-yaml": "^4.0.5",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.42",
    "concurrently": "^7.2.1",
    "jest": "^28.1.1",
    "nodemon": "^2.0.16",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.3"
  }
}
